#import "SubTableViewStyleCell.h"

@implementation SubTableViewStyleCell

@synthesize subTableView = _subTableView;

- (id)initWithSubTableViewClass:(Class)aClass reuseIdentifier:(NSString *)reuseIdentifier cellSize:(CGSize)size;
{
    self = [super initWithStyle:UITableViewCellStyleDefault reuseIdentifier:reuseIdentifier];
    if (self) {
        // Initialization code
        
        _subTableView = [[aClass alloc] initWithFrame:CGRectMake(0, 0, size.width, size.height) style:UITableViewStylePlain];
        
        [self.contentView addSubview:_subTableView];
        
        [_subTableView release];
        
        self.selectionStyle = UITableViewCellSelectionStyleNone;
        
    }
    return self;
}

- (void)setSelected:(BOOL)selected animated:(BOOL)animated
{
    [super setSelected:selected animated:animated];

    // Configure the view for the selected state
}

- (void)dealloc {
    
    _subTableView = nil;
    
    [super dealloc];
}

@end
